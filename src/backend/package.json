{
  "name": "smartpark-backend",
  "version": "1.0.0",
  "description": "Smart Park Management System Backend",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "db:migrate": "ts-node src/db/migrate.ts",
